package Maiorvalor;
import java.util.Scanner;
public class maior {
	public static void main(String[] args) {
		int numeros=0;
		int cont=0;
		int maior=0;
		Scanner s = new Scanner(System.in);
		System.out.println("Entre com numeros :");
	
		for(int tam=0;tam<=14;tam++) {
		numeros = s.nextInt();
		if (numeros>maior) {
			maior=numeros;	
		}
		cont++;
		}		
		s.close();
		System.out.println("\nO maior numero digitado e de: "+ maior + "\nForam digitados " + cont + " numeros." );
		
	}
}
